{
  "name": "f-this",
  "description": "Create filters for web pages",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "calculator-16.png",
    "default_popup": "window.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "http://f-this.appspot.com/*"
  ],
  // "background": {
  //   "scripts": ["window.js"]
  // },
  "content_scripts": [{
    "matches": ["http://*/*"],
    "js": ["jquery-2.1.4.js", "findAndReplace.js"]
  }],
  "icons": { "16": "calculator-16.png", "128": "calculator-128.png" }
}